<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="videoclub.videoclubapp.LoginController">
   <children>
      <ImageView fx:id="imgLogo" fitHeight="400.0" fitWidth="344.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="labelUser" layoutX="359.0" layoutY="112.0" prefHeight="25.0" prefWidth="50.0" text="User">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtUser" layoutX="359.0" layoutY="150.0" />
      <Label fx:id="labelPass" layoutX="359.0" layoutY="195.0" prefHeight="27.0" prefWidth="90.0" text="Password">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="txtPass" layoutX="359.0" layoutY="231.0" />
      <Button fx:id="btnLogin" layoutX="403.0" layoutY="299.0" mnemonicParsing="false" onAction="#login" text="Login">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
